<%= form_with model: upload do |form| %>
  <div>
    <%= form.label :photo%><br>
    <%= form.file_field :photo ,class:"form-control" %>
    
  </div>

  <div>
    <%= form.label :info %><br>
    <%= form.text_area :info ,class:"form-control"%><br>
    
  </div>

    <div>
    <%= form.hidden_field :uploaded_by, value: current_user.name %><br>
  </div>

  <div>
    <%= form.submit class:"btn btn-success"%>
  </div>
<% end %>
